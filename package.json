{
  "name": "spritespin",
  "description": "jQuery plugin for creating flipbook animations",
  "version": "5.0.0",
  "author": "Alexander Graefenstein",
  "license": "MIT",
  "keywords": [
    "spritespin",
    "360",
    "animation",
    "flipbook",
    "panorama",
    "jquery-plugin",
    "ecosystem:jquery"
  ],
  "main": "release/spritespin.js",
  "module": "release/src/index.js",
  "typings": "release/src/index.d.ts",
  "es2015": "release/esm2015/index.js",
  "files": [
    "release",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/giniedp/spritespin.git"
  },
  "bugs": {
    "url": "https://github.com/giniedp/spritespin/issues"
  },
  "engines": {
    "node": "12"
  },
  "scripts": {
    "lint": "eslint -c .eslintrc.js",
    "lint:silent": "eslint -c .eslintrc.js || true",
    "build": "tsc --project lib/tsconfig.json",
    "build:es5": "tsc --project lib/tsconfig.json",
    "build:api": "api-extractor run --local --verbose",
    "build:cjs": "tsc --project lib/tsconfig.cjs.json",
    "build:tsc": "tsc --project lib/tsconfig.json",
    "build:esm2015": "tsc --project tsconfig.esm2015.json",
    "build:watch": "yarn build -w",
    "build:rollup": "rollup -c rollup.config.js",
    "build:rollup:watch": "yarn build:rollup -w",
    "build:page": "ts-node pages.ts",
    "build:page:watch": "yarn build:page -w",
    "build:docs": "api-documenter markdown -i temp -o docs",
    "test": "karma start --no-auto-watch --single-run",
    "test:watch": "karma start --auto-watch --no-single-run",
    "test:coverage": "IS_COVERAGE=yes karma start --single-run",
    "test:coveralls": "IS_COVERALLS=yes karma start --single-run && cat coverage/lcov/lcovonly | ./node_modules/.bin/coveralls",
    "dev": "concurrently 'yarn build:page:watch' 'yarn build:rollup:watch'"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@microsoft/api-documenter": "^7.12.16",
    "@microsoft/api-extractor": "7.13.2-pr1916.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/gaze": "^1.1.0",
    "@types/jasmine": "^3.6.9",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "bootstrap": "^5.0.0-beta3",
    "browser-sync": "^2.26.14",
    "chokidar": "^3.5.1",
    "concurrently": "^6.0.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "jasmine-core": "^3.7.1",
    "jstransformer-markdown-it": "^2.1.0",
    "jstransformer-prismjs": "^0.1.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-jasmine": "4.0.1",
    "karma-mocha-reporter": "^2.2.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-typescript": "^5.5.1",
    "metalsmith": "^2.3.0",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-filter": "^1.0.2",
    "metalsmith-layouts": "^2.3.1",
    "metalsmith-markdown": "^1.3.0",
    "metalsmith-permalinks": "^2.2.0",
    "metalsmith-sass": "^1.7.0",
    "pug": "^3.0.2",
    "pug-cli": "^1.0.0-alpha6",
    "rollup": "^2.44.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.20.34",
    "typescript": "^4.2.3",
    "webpack-log": "^3.0.2"
  },
  "dependencies": {
    "@types/metalsmith": "^2.3.0"
  }
}
